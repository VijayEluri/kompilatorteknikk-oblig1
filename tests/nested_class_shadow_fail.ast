			(CLASS (NAME Bogus)
				(VAR_DECL (TYPE int)(NAME innerScope))
			)
			(VAR_DECL (TYPE (NAME Bogus))(NAME inner))
			(ASSIGN_STMT
				(NAME inner)
				(NEW (TYPE (NAME Bogus)))
)

			(ASSIGN_STMT
				( . (NAME inner) (NAME outerScope))
				(INT_LITERAL 666)
)

		)
