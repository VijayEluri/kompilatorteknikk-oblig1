			(NAME outer)
			(NEW (TYPE (NAME Bogus)))
)

		(ASSIGN_STMT
			( . (NAME outer) (NAME outerScope))
			(INT_LITERAL 1337)
)

		(CALL_STMT (NAME dummy)

	)

)
